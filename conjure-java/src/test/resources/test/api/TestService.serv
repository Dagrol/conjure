package test.api;

import com.palantir.foundry.catalog.api.CreateDatasetRequest;
import com.palantir.foundry.catalog.api.datasets.BackingFileSystem;
import com.palantir.foundry.catalog.api.datasets.Dataset;
import com.palantir.ri.ResourceIdentifier;
import com.palantir.tokens.AuthHeader;
import java.lang.String;
import java.util.Map;
import java.util.Optional;
import java.util.Set;
import retrofit2.http.Body;
import retrofit2.http.Call;
import retrofit2.http.GET;
import retrofit2.http.Header;
import retrofit2.http.POST;
import retrofit2.http.Path;

/**
 * A Markdown description of the service.
 */
public interface TestService {
    /**
     * Returns a mapping from file system id to backing file system configuration.
     */
    @GET("/fileSystems")
    Call<Map<String, BackingFileSystem>> getFileSystems(@Header("Authorization") AuthHeader authHeader);

    @POST("/datasets")
    Call<Dataset> createDataset(@Header("Authorization") AuthHeader authHeader, @Body CreateDatasetRequest request);

    @GET("/datasets/{datasetRid}")
    Call<Optional<Dataset>> getDataset(@Header("Authorization") AuthHeader authHeader, @Path("datasetRid") ResourceIdentifier datasetRid);

    @GET("/datasets/{datasetRid}/branches")
    Call<Set<String>> getBranches(@Header("Authorization") AuthHeader authHeader, @Path("datasetRid") ResourceIdentifier datasetRid);
}
