package test.api;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;
import com.fasterxml.jackson.annotation.JsonProperty;
import java.lang.Integer;
import java.lang.Object;
import java.lang.Override;
import java.lang.String;
import java.util.Objects;
import java.util.Optional;

@JsonIgnoreProperties(
        ignoreUnknown = true
)
public final class SimpleObject {
    private final String stringField;

    private final Optional<Integer> optionalField;

    public SimpleObject(@JsonProperty("stringField") String stringField, @JsonProperty("optionalField") Optional<Integer> optionalField) {
        this.stringField = stringField;
        this.optionalField = optionalField;
    }

    public String getStringField() {
        return this.stringField;
    }

    public Optional<Integer> getOptionalField() {
        return this.optionalField;
    }

    @Override
    public boolean equals(Object other) {
        return this == other || (other instanceof SimpleObject && equalTo((SimpleObject) other));
    }

    private boolean equalTo(SimpleObject other) {
        return Objects.equals(this.stringField, other.stringField) && Objects.equals(this.optionalField, other.optionalField);
    }

    @Override
    public int hashCode() {
        return Objects.hash(stringField, optionalField);
    }
}
