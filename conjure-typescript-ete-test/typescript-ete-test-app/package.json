{
  "name": "typescript-ete-test",
  "version": "0.1.0",
  "description": "A Highline project",
  "private": true,
  "scripts": {
    "build": "npm-run-all -p copy:prod compile -s bundle:prod",
    "buildDev": "npm-run-all -p copy:dev compile -s bundle:dev ",
    "clean": "rm -rf build/",
    "npmShrinkwrap": "../scripts/npmShrinkwrap.sh",
    "start": "npm-run-all -p copy:dev compile -p watch:typescript watch:server",
    "tdd": "npm-run-all compile:test -p watch:typescript:test test:karma:unit",
    "etetdd": "npm-run-all compile:test -p watch:typescript:test etetest:karma:unit",
    "test": "npm-run-all compile:test test:karma:cov",
    "etetest": "npm-run-all compile:test etetest:karma:cov",
    "watch": "npm-run-all -p copy:dev compile -p watch:bundle watch:typescript",
    "bundle:dev": "webpack -d",
    "bundle:prod": "webpack --config webpack.config.prod.js",
    "compile": "npm-run-all -p lint compile:typescript",
    "compile:test": "npm-run-all test:tsconfig -p lint:test compile:typescript:test",
    "compile:typescript": "tsc -p .",
    "compile:typescript:test": "tsc -p test",
    "copy:dev": "rsync -av --exclude=*.css --exclude=*.js --exclude=index.html --exclude=*.less --exclude=*.ts --exclude=*.tsx src/ build/src",
    "copy:prod": "rsync -av --exclude=*.css --exclude=*.js --exclude=index.html --exclude=*.less --exclude=*.ts --exclude=*.tsx src/ build/min",
    "lint": "npm-run-all -p lint:eslint lint:stylelint lint:tslint",
    "lint:test": "npm-run-all -p lint lint:tslint:test",
    "lint:eslint": "eslint *.js",
    "lint:stylelint": "stylelint -s less 'src/**/*.less'",
    "lint:tslint": "tslint -c tslint.json 'src/**/*.ts*' -e 'src/conjure/**/*.ts*'",
    "lint:tslint:test": "tslint -c tslint.json 'test/src/**/*.ts*'",
    "test:karma:cov": "echo 'No tests'",
    "etetest:karma:cov": "karma start karma.cov.conf.js",
    "test:karma:unit": "echo 'No tests'",
    "etetest:karma:unit": "karma start karma.unit.conf.js",
    "test:tsconfig": "tsconfig test",
    "watch:bundle": "webpack -d -w",
    "watch:server": "webpack-dev-server -d --config webpack.config.server.js",
    "watch:typescript": "tsc -w -p .",
    "watch:typescript:test": "tsc -w -p test"
  },
  "author": "Palantir Technologies, Inc.",
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "eslint": "3.1.1",
    "eslint-config-palantir": "1.0.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "handlebars": "4.0.5",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "js-yaml": "3.6.1",
    "karma": "1.1.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.0.5",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-phantomjs-shim": "1.4.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "2.5.3",
    "npm": "3.10.5",
    "npm-run-all": "2.3.0",
    "phantomjs-prebuilt": "2.1.8",
    "postcss-less": "0.14.0",
    "postcss-loader": "0.9.1",
    "sinon": "1.17.5",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "stylelint": "7.0.3",
    "stylelint-config-palantir": "1.1.0",
    "tsconfig-glob": "0.4.3",
    "tslint": "3.14.0",
    "tslint-loader": "2.1.5",
    "tslint-react": "0.4.0",
    "typescript": "1.8.10",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "@elements/blueprint": "2.30.0",
    "@elements/conjure-fe-lib": "0.2.0",
    "es6-promise": "3.2.1",
    "lodash": "4.14.0",
    "plottable": "2.2.0",
    "react": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "1.0.0"
  }
}
